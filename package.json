{
  "name": "eyogi",
  "version": "1.0.0",
  "description": "Website template for Payload",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "build": "cross-env NODE_OPTIONS=--no-deprecation next build",
    "postbuild": "next-sitemap --config next-sitemap.config.cjs",
    "dev": "cross-env NODE_OPTIONS=--no-deprecation next dev --turbo",
    "dev:prod": "cross-env NODE_OPTIONS=--no-deprecation rm -rf .next && pnpm build && pnpm start",
    "generate:importmap": "cross-env NODE_OPTIONS=--no-deprecation payload generate:importmap",
    "generate:types": "cross-env NODE_OPTIONS=--no-deprecation payload generate:types",
    "ii": "cross-env NODE_OPTIONS=--no-deprecation pnpm --ignore-workspace install",
    "lint": "cross-env NODE_OPTIONS=--no-deprecation next lint",
    "lint:fix": "cross-env NODE_OPTIONS=--no-deprecation next lint --fix",
    "payload": "cross-env NODE_OPTIONS=--no-deprecation payload",
    "reinstall": "cross-env NODE_OPTIONS=--no-deprecation rm -rf node_modules && rm pnpm-lock.yaml && pnpm --ignore-workspace install",
    "start": "cross-env NODE_OPTIONS=--no-deprecation next start",
    "deploy": "node deploy.js",
    "netlify:build": "npm run build",
    "netlify:dev": "npx netlify dev",
    "netlify:deploy": "npx netlify deploy --prod --dir=.next",
    "netlify:preview": "npx netlify deploy --dir=.next"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.10.0",
    "@payloadcms/db-postgres": "3.9.0",
    "@payloadcms/live-preview-react": "3.9.0",
    "@payloadcms/next": "3.9.0",
    "@payloadcms/payload-cloud": "3.9.0",
    "@payloadcms/plugin-form-builder": "3.9.0",
    "@payloadcms/plugin-nested-docs": "3.9.0",
    "@payloadcms/plugin-redirects": "3.9.0",
    "@payloadcms/plugin-search": "3.9.0",
    "@payloadcms/plugin-seo": "3.9.0",
    "@payloadcms/richtext-lexical": "3.9.0",
    "@payloadcms/storage-uploadthing": "3.9.0",
    "@payloadcms/ui": "3.9.0",
    "@radix-ui/react-accordion": "^1.2.11",
    "@radix-ui/react-checkbox": "^1.0.4",
    "@radix-ui/react-dialog": "^1.1.13",
    "@radix-ui/react-label": "^2.1.1",
    "@radix-ui/react-navigation-menu": "^1.2.12",
    "@radix-ui/react-select": "^2.0.0",
    "@radix-ui/react-separator": "^1.1.1",
    "@radix-ui/react-slot": "^1.2.2",
    "@radix-ui/react-tabs": "^1.1.9",
    "@radix-ui/react-toast": "^1.2.5",
    "@tabler/icons-react": "^3.33.0",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "cross-env": "^7.0.3",
    "embla-carousel-react": "^8.6.0",
    "framer-motion": "^12.4.7",
    "geist": "^1.3.0",
    "graphql": "^16.8.2",
    "lenis": "^1.1.21",
    "lucide-react": "^0.378.0",
    "matter-js": "^0.20.0",
    "motion": "^12.15.0",
    "next": "^15.1.0",
    "next-sitemap": "^4.2.3",
    "next-transition-router": "^0.2.7",
    "payload": "3.9.0",
    "payload-admin-bar": "^1.0.6",
    "prism-react-renderer": "^2.3.1",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-hook-form": "^7.54.2",
    "react-lite-youtube-embed": "^2.5.1",
    "react-matter-js": "^1.0.5",
    "resend": "^4.1.2",
    "sharp": "0.32.6",
    "tailwind-merge": "^2.3.0",
    "tailwind-scrollbar": "^4.0.2",
    "tailwindcss-animate": "^1.0.7",
    "uploadthing": "^7.4.4",
    "zod": "^3.24.1",
    "autoprefixer": "^10.4.19",
    "postcss": "^8.4.38",
    "tailwindcss": "^3.4.3",
    "@tailwindcss/typography": "^0.5.13"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "@types/escape-html": "^1.0.2",
    "@types/matter-js": "^0.19.8",
    "@types/node": "22.5.4",
    "@types/react": "19.0.1",
    "@types/react-dom": "19.0.1",
    "copyfiles": "^2.4.1",
    "eslint": "^9.16.0",
    "eslint-config-next": "15.1.0",
    "prettier": "^3.4.2",
    "typescript": "5.7.2"
  },
  "engines": {
    "node": "^18.20.2 || >=20.9.0"
  }
}
